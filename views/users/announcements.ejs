<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("../partials/head")  %> 
</head>
<body>
    
    <%- include("../partials/nav")  %> 

    <div class="container mt-3">
        <div class="row">
            <div class="col-md-3">
                <%- include("../partials/club-menu", {clubs, selectedClub})  %> 
            </div>
            <div class="col-md-9">
                <h1 class="h3 mb-3">
                    <%= title %>
                </h1>

                <% if (announcements.length == 0) { %>
                    <%- include("../partials/no-products")  %>
                <% } %>

                <% announcements.forEach(announcement => { %>
                        <%- include("../partials/news-card", announcement)  %> 
                <% }) %>

                    <%- include("../partials/pagination")  %> 
                              
            </div>
        </div>
    </div>

</body>
</html>



